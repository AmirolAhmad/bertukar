.row-fluid
  .align-center.well
    #searchbox
      = search_form_for @q, url: search_index_path, html: { method: :get, class: "form-inline" } do |f|
        = f.text_field :gred_eq, class: "span1", placeholder: "F41"
        = f.collection_select :location_id_or_location_state_id_eq, Location.states, :id, :nama, { include_blank: true }
        = f.submit "Buat Carian", class: "btn btn-primary"
