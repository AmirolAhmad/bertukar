= form_for @admin_location do |f|
  -if @admin_location.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_location.errors.count, "error")} prohibited this admin_location from being saved:"
      %ul
        - @admin_location.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :state_id
    = f.number_field :state_id
  .field
    = f.label :nama
    = f.text_field :nama
  .actions
    = f.submit 'Save'
