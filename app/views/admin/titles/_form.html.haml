= form_for @admin_title do |f|
  -if @admin_title.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_title.errors.count, "error")} prohibited this admin_title from being saved:"
      %ul
        - @admin_title.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nama
    = f.text_field :nama
  .actions
    = f.submit 'Save'
